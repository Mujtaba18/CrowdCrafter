<table>
<div>location: <%= location.names %></div>
<div>
  capacity: <%= location.capacity %>
</div>

<div>
  availability: <%= location.availability %>
</div>

<div>
  city: <%= location.city %>
</div>

<div>E-mail: <%= location.bookingEmail %></div>

<div>
  phone number: <%= location.bookingPhone %>
</div>

<div>indoor: <%= location.indoor %></div>
<div>
    <div>
      <img
        src="data:location/<%=location.img.contentType%>;base64,
        <%=location.img.data.toString('base64')%>"
      />
      <div>
        <h5><%= location.names %></h5>
      </div>
    </div>
</div>


</table>